<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="feature.js" verb="*" modules="iisnode" />
    </handlers>    
   <rewrite>
     <rules>
        <rule name="feature">
           <match url="feature/*" />
           <action type="Rewrite" url="feature.js" />
        </rule>
      </rules>
   </rewrite>

  </system.webServer>
</configuration>

