<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="lib/app.js" verb="*" modules="iisnode" />
    </handlers>    
   <rewrite>
     <rules>
        <rule name="app">
           <match url="feature*/*" />
           <action type="Rewrite" url="lib/app.js" />
        </rule>
      </rules>
   </rewrite>
  </system.webServer>
</configuration>

